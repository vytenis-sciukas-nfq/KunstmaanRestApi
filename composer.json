{
    "name": "kunstmaan/rest",
    "type": "symfony-bundle",
    "description": "The Kunstmaan Rest Api Bundles provides a REST api for the popular Kunstmaan CMS bundles.",
    "keywords": [
        "kunstmaan",
        "cms",
        "rest",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "kunstmaan/config-bundle": "^5.2|^6.0",
        "kunstmaan/dashboard-bundle": "5.2|^6.0",
        "kunstmaan/menu-bundle": "5.2|^6.0",
        "kunstmaan/node-search-bundle": "5.2|^6.0",
        "kunstmaan/redirect-bundle": "5.2|^6.0",
        "kunstmaan/search-bundle": "5.2|^6.0",
        "kunstmaan/article-bundle": "5.2|^6.0",
        "kunstmaan/user-management-bundle": "^5.2|^6.0",
        "friendsofsymfony/rest-bundle": "^3.1",
        "jms/serializer-bundle": "^3.10",
        "ramsey/uuid": "^3.8",
        "nelmio/api-doc-bundle": "~4.1.0",
        "willdurand/hateoas": "^3.7",
        "willdurand/hateoas-bundle": "^2.3"
    },
    "require-dev": {
        "nelmio/alice": "^2.1.4"
    },
    "autoload": {
        "psr-4": {
            "Kunstmaan\\Rest\\": "src/Rest/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
